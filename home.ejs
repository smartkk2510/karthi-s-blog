<%- include("./header") -%>
<h1>Home</h1>
<p><%=startingContent%></p>
<% posts.forEach((post) => { %>
    <h2><%=post.title%></h2>
     <!-- these are js values so we have to use <%= %> -->
    <p><%= post.content.substring(0,100) + " ..."%>
        <a href="/posts/<%= post.title %>">Read More</a>
    </p>
<% }); %>
<%- include("./footer") -%>
